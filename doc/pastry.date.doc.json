[{
    "description": "extend for Date",
    "filename": "pastry.date.js",
    "requires": [""]
}, {
    "description": "return stringified date according to given pattern.",
    "param": [{
        "type": "string",
        "name": "pattern",
        "description": "defines pattern for stringify."
    }],
    "return": {
        "type": "string",
        "description": "result string."
    },
    "syntax": "date.stringf([pattern])",
    "example": ["'{YYYY}-{MM}-{DD} {hh}:{mm}:{ss}' => '2013-10-03 00:57::13'", "'{YY}-{M}-{D} {h}:{m}:{s}' => '13-10-3 0:57::13'"]
}]