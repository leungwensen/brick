[
  {
    "description": "extend for JSON",
    "filename": "pastry.json.js",
    "requires": [
      "pastry.js",
      "pastry.array.js",
      "pastry.date.js"
    ],
    "refference": {
      "type": "link",
      "url": "https://github.com/douglascrockford/JSON-js/blob/master/json2.js"
    }
  },
  {
    "description": "stringify a JSON object.",
    "param": [
      {
        "type": "unknown",
        "name": "value",
        "description": "value to be stringified"
      }
    ],
    "return": [
      {
        "type": "string",
        "description": "result string."
      }
    ],
    "syntax": "PT.JSON.stringify(value)."
  },
  {
    "description": "parse a string to JSON object",
    "param": [
      {
        "type": "string",
        "name": "string",
        "description": "string to parse"
      }
    ],
    "return": [
      {
        "type": "unknown",
        "description": "result object."
      }
    ],
    "syntax": "PT.JSON.parse(string)."
  },
  {}
]