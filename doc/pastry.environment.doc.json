[
  {
    "description": "about running enviroment",
    "filename": "pastry.enviroment.js",
    "requires": [
      "pastry.js",
      "pastry.array.js",
      "pastry.string.js"
    ]
  },
  {
    "description": "init PT.PL | PT.PLUG | PT.VER for current enviroment",
    "syntax": "PT.initUA()"
  },
  {
    "description": "platform",
    "alias": "PT.platform",
    "syntax": "PT.PL"
  },
  {
    "description": "plugins",
    "alias": "PT.plugins",
    "syntax": "PT.PLUG"
  },
  {
    "description": "versions of browser or nodejs",
    "alias": "PT.versions",
    "syntax": "PT.VER"
  },
  {
    "description": "host of the window",
    "syntax": "PT.HOST"
  },
  {
    "description": "document of the window",
    "syntax": "PT.DOC"
  },
  {
    "description": "userAgent of the browser",
    "alias": "PT.userAgent",
    "syntax": "PT.UA"
  },
  {
    "description": "detect platform",
    "param": [
      {
        "type": "string",
        "name": "platformStr",
        "description": "platform defined string."
      }
    ],
    "syntax": "PT.detectPL(platformStr)",
    "return": [
      {
        "type": "string",
        "description": "platform. (mac|windows|linux...)"
      }
    ]
  },
  {
    "description": "detect plugins (now flash only)",
    "param": [
      {
        "type": "array",
        "name": "plugins",
        "description": "plugin list"
      }
    ],
    "syntax": "PT.detectPLUG(plugins)",
    "return": [
      {
        "type": "object",
        "description": "{ 'flash' : 0|xx }"
      }
    ]
  },
  {
    "description": "detect versions (nodejs: v8 version, node version ,.. ; browsers: browser version, platform version, ...)",
    "param": [
      {
        "type": "string",
        "name": "userAgent",
        "description": "window.navigator.userAgent"
      }
    ],
    "syntax": "PT.detectVer(userAgent)",
    "return": [
      {
        "type": "object",
        "description": "{ 'flash' : 0|xx }"
      }
    ]
  },
  "browser versions",
  "chrome",
  "safari version",
  "mobile safari version",
  "engine versions"
]